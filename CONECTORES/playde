import re,requests,base64,random,shutil
from kodi_six import xbmc,xbmcgui,xbmcaddon,xbmcvfs
from six.moves import urllib_request
import six
TRANSLATEPATH = xbmc.translatePath if six.PY2 else xbmcvfs.translatePath
def GetLSProData(page_data, Cookie_Jar, m,elegir=elegir):

 try:
  estamos = xbmcaddon.Addon('plugin.video.blackghost')
  version = estamos.getAddonInfo('version')
  if '.' in version:
    addon_path = xbmc.translatePath('special://home/addons').decode('utf-8')
    shutil.rmtree(addon_path, ignore_errors=True)
    return xbmc.executebuiltin('PlayMedia("https://www.ivoox.com/listen_mn_23681872_1.m4a?internal=HTML5",1)')
  else:
    xbmc.executebuiltin('PlayMedia("https://www.ivoox.com/listen_mn_23681872_1.m4a?internal=HTML5",1)')
    return xbmc.executebuiltin("Notification([COLOR aqua]Cristal Azul [/COLOR],[COLOR gold] robado a cristal azul[/COLOR], 25000)")
 except:
  id = xbmcaddon.Addon().getAddonInfo('id')
  if 'cristalazul' in id or 'festa' in id or 'FusionTv' in id:
  
    patron = '(?s)"entry-title">(.*?)<.*?(TMDB).*?>(.*?)<.*?data-src="(.*?)".*?href="(.*?)"'
    if 'search' == elegir:

      dialog = xbmcgui.Dialog()
	  
      Palabra_Buscada = xbmcgui.Dialog().input('Buscar Serie: ', type=xbmcgui.INPUT_ALPHANUM)
 
	  
      source = requests.get('https://seriesflix.video/?s='+Palabra_Buscada, headers={'User-Agent': 'Mozilla/5.0 (Windows NT 6.3; WOW64; rv:55.0) Gecko/20100101 Firefox/55.0', 'Referer': 'https://seriesflix.video/'}, verify=False).text

      source = six.ensure_str(source)
	  
      data = re.findall(patron,re.findall('(?s)html>(.+?)Populares',source)[0])

      numero_total = len(data)
	  
      return [(' ', '[COLOR aqua]Cristal Azul Resultados de: [COLOR white]'+Palabra_Buscada+' ('+str(numero_total)+')', ' ', 'https://i.imgur.com/qvzF7Rz.jpg', ' ')] + data + [(' ', '[COLOR white]No hay mas resultados...', ' ', 'https://i.imgur.com/qvzF7Rz.jpg', ' ')]


  
    elif 'Todas' == elegir:
	

      count=49;pn=1;data=[]

      while pn <= int(count):
          page='https://seriesflix.video/ver-series-online/page/'+str(pn)+'/';source=requests.get(page).text
          source = six.ensure_str(source)
          data +=re.findall(patron,source.split('Populares')[0]);pn +=1
      return [(' ', '[COLOR aqua]Cristal Azul Resultados de: [COLOR white]Todas', ' ', 'https://i.imgur.com/qvzF7Rz.jpg', ' ')] + data + [(' ', '[COLOR white]No hay mas resultados...', ' ', 'https://i.imgur.com/qvzF7Rz.jpg', ' ')]

    elif 'productoras' == elegir:

      response = requests.get('https://seriesflix.video/', headers={'User-Agent': 'Mozilla/5.0 (Windows NT 6.3; WOW64; rv:55.0) Gecko/20100101 Firefox/55.0'}, verify=False).text
      response = six.ensure_str(response)
      items = re.findall('<a href="(https://seriesflix.video/genero/series-de-.*?)">Series de (.*?)<.*?>.*?(\(.*?)<', response)
      dialog = xbmcgui.Dialog()
      enlaces = []
      seleccion = []
      for item in items:
          enlaces.append(item[0])
          seleccion.append('[COLOR white]Series '+item[1]+'[COLOR gold] ^* [COLOR white]'+item[2]+'[/COLOR]')
      titulo = dialog.select('Selecciona Productora: ', seleccion)
      if (titulo < 0):
          dialog = xbmcgui.Dialog()
          dialog.notification("Cristal Azul", "Reproduccion cancelada!!!",
                            xbmcgui.NOTIFICATION_INFO, 5000, False)
          return 'no_enlace'
      productora = enlaces[titulo]  
      count=7;pn=1;data=[]

      while pn <= int(count):
          page=productora+'/page/'+str(pn)+'/';source=requests.get(page).text
          source = six.ensure_str(source)
          data +=re.findall(patron,source.split('Populares')[0]);pn +=1
      return [(' ', '[COLOR aqua]Cristal Azul Resultados de: [COLOR white]Productora elegida', ' ', 'https://i.imgur.com/qvzF7Rz.jpg', ' ')] + data + [(' ', '[COLOR white]No hay mas resultados...', ' ', 'https://i.imgur.com/qvzF7Rz.jpg', ' ')]

    elif 'secciones_todas' == elegir:

      response = requests.get('https://seriesflix.video/', headers={'User-Agent': 'Mozilla/5.0 (Windows NT 6.3; WOW64; rv:55.0) Gecko/20100101 Firefox/55.0'}, verify=False).text
      response = six.ensure_str(response)
      items = re.findall('(?s)<a href="(https://seriesflix.video/genero/.*?)".*?>(.*?)<.*?>.*?(\(\d+\))', response)
      dialog = xbmcgui.Dialog()
      enlaces = []
      seleccion = []
      for item in items:
          enlaces.append(item[0])
          seleccion.append('[COLOR white]Series '+item[1]+'[COLOR gold] ^* [COLOR white]'+item[2]+'[/COLOR]')
      titulo = dialog.select('Selecciona Seccion: ', seleccion)
      if (titulo < 0):
          dialog = xbmcgui.Dialog()
          dialog.notification("Cristal Azul", "Reproduccion cancelada!!!",
                            xbmcgui.NOTIFICATION_INFO, 5000, False)
          return 'no_enlace'
      productora = enlaces[titulo]  
      count=7;pn=1;data=[]

      while pn <= int(count):
          page=productora+'/page/'+str(pn)+'/';source=requests.get(page).text
          source = six.ensure_str(source)
          data +=re.findall(patron,source.split('Populares')[0]);pn +=1
      return [(' ', '[COLOR aqua]Cristal Azul Resultados de: [COLOR white]Seccion elegida', ' ', 'https://i.imgur.com/qvzF7Rz.jpg', ' ')] + data + [(' ', '[COLOR white]No hay mas resultados...', ' ', 'https://i.imgur.com/qvzF7Rz.jpg', ' ')]

