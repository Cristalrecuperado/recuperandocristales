      lists = ['accion', 'animacion', 'aventura', 'series-antiguas', 'drama', 'ciencia-ficcion', 'comedia', 'crimen', 'terror', 'dorama', 'dc-comics', 'fantasia', 'anime']
 
      genero = lists[ret]
  
      count=15;pn=1;data=[]

